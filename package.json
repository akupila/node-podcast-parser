    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "1.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "sax": "1.1.5"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akupila/node-podcast-parser"
